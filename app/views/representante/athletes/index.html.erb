<h1>Listing representante_athletes</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Sex</th>
    <th>Age</th>
    <th>Organization</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @representante_athletes.each do |representante_athlete| %>
  <tr>
    <td><%= representante_athlete.name %></td>
    <td><%= representante_athlete.sex %></td>
    <td><%= representante_athlete.age %></td>
    <td><%= representante_athlete.organization %></td>
    <td><%= link_to 'Show', representante_athlete %></td>
    <td><%= link_to 'Edit', edit_representante_athlete_path(representante_athlete) %></td>
    <td><%= link_to 'Destroy', representante_athlete, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

